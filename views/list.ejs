<h1>My shopping list:</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i<users.length; i++) { %>
      <tr>
        <td><%= users[i].name %></td>
        <td><%= users[i].age %></td>
        <td><%= users[i].email %></td>
      </tr>
    <% } %>
  </tbody>
</table>
<form action="/" method="post">
  <input type="text" name="text" /><input type="submit" value="Submit" />
</form>
